#
# cmd/mkmsgs/Makefile
#
PROG=		adm_mkmsgs

SHFILES =

TXTS =

OBJECTS=	mkmsgs.o

SRCS=		$(OBJECTS:.o=.c)

include ../Makefile.cmd

# where to find the libraries
# what gets installed, and where

.KEEP_STATE:

all:		$(PROG)

$(PROG):	$(OBJECTS)
		$(LINK.c) -o $@ $(OBJECTS) $(LDLIBS)
		$(POST_PROCESS)

install:	all $(ROOTADMINBINPROG)

install_h:

lint:		lint_SRCS

clean:
	$(RM) $(OBJECTS)

include ../Makefile.targ
